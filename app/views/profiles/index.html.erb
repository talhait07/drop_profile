<h1>Listing profiles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Title</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Phone</th>
      <th>Status</th>
      <th>Web link</th>
      <th>Facebook link</th>
      <th>Twitter link</th>
      <th>Linkedin link</th>
      <th>Google plus link</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.user_id %></td>
        <td><%= profile.title %></td>
        <td><%= profile.address %></td>
        <td><%= profile.city %></td>
        <td><%= profile.state %></td>
        <td><%= profile.country %></td>
        <td><%= profile.phone %></td>
        <td><%= profile.status %></td>
        <td><%= profile.web_link %></td>
        <td><%= profile.facebook_link %></td>
        <td><%= profile.twitter_link %></td>
        <td><%= profile.linkedin_link %></td>
        <td><%= profile.google_plus_link %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
